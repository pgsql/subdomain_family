<div class="edit_content_head"> 	<div class="spacer5"></div>	<strong>New Master Account</strong></div><% if @user.errors.any? %>   <div id="error_explanation">	  <ul>	      <% @user.errors.full_messages.each do |msg| %>	         <li><%= msg %></li>	      <% end %>	  </ul>   </div><% end %><div class="forms_container">     <%= form_for(@user, :url => {:action => "create_master_account"}, :method => :post ) do |f| %>             <div class="forms_container_box">				 <div class="spacer5"></div>				 <div class="titlebox_starts">					<div class="titlebox_one"> <%= f.label :email %> </div>					<div class="titlebox_two">  <%= f.email_field :email %> </div>				 </div>				 <div class="titlebox_starts">					<div class="titlebox_one"> <%= f.label :password %> </div>					<div class="titlebox_two"> <%= f.password_field :password %> </div>				 </div>				 <div class="titlebox_starts">					<div class="titlebox_one"> <%= f.label :password_confirmation %> </div>					<div class="titlebox_two"> <%= f.password_field :password_confirmation %> </div>				 </div>				 <div class="titlebox_starts">					<div class="titlebox_one">  <%= f.label :type %> </div>					<div class="titlebox_two"> <%= f.select :type, options_for_select(%w(Doctor Patient Staff Company)) %> </div>				 </div>		   </div>		   <div class="update_but">			  <%= f.submit %> Or <span class="cancel"> <%= link_to 'Cancel', users_path %> </span>		   </div>    <% end %>	</div>