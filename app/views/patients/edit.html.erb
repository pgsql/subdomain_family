<div class="edit_content_head">     <div class="spacer5"></div>    <strong>Edit Account</strong></div>			  <% if @user.errors.any? %>				  <div id="error_explanation">					  <ul>						  <% @user.errors.full_messages.each do |msg| %>							 <li><%= msg %></li>						  <% end %>					  </ul>				  </div>			  <% end %>			  <div class="forms_container">       <%= form_for(@user, :url => patient_path(@user), :method => :put ) do |f| %>           <div class="forms_container_box">			                    				       <div class="titlebox_starts">					      <div class="titlebox_one"> <%= f.label :title %> </div>						  <div class="titlebox_two"> <%= f.select :title, options_for_select(%w(MR MS MRS )) %> </div>					   </div>					   <div class="titlebox_starts">						   <div class="titlebox_one"> <%= f.label :first_name %> </div>						   <div class="titlebox_two"> <%= f.text_field :first_name %> </div>					   </div>                       <div class="titlebox_starts">						   <div class="titlebox_one"> Surname </div>						   <div class="titlebox_two"> <%= f.text_field :last_name %> </div>                       </div>	                  <br/>				  				        <div class="personal_details"></div> <br/>			            <%= fields_for :user_detail do |user_detail| %>                             <div class="birth_titlebox_starts">                                <div class="birth_titlebox_one"> Date Of Birth </div>                                <div class="birth_titlebox_two"><%= datetime_select("user_detail", "date_of_birth", :start_year => 1995) %></div>                             </div>			            <% end %>					         <div class="titlebox_starts">						        <div class="titlebox_one"> Residential Address </div>						        <div class="titlebox_two"> <%= f.text_field :address %> </div>					         </div>			            <%= fields_for :user_detail do |user_detail| %>										         <div class="titlebox_starts">						        <div class="titlebox_one"> Postal Address </div>						        <div class="titlebox_two"> <%= user_detail.text_field :postal_address %> </div>					         </div>                        <% end %>			 			                 <div class="titlebox_starts">						        <div class="titlebox_one"> Home Phone </div>						        <div class="titlebox_two"> <%= f.text_field :home_number %> </div>					         </div>					         <div class="titlebox_starts">						         <div class="titlebox_one">  Work Phone </div>						         <div class="titlebox_two"> <%= f.text_field :office_number %> </div>    					     </div>					         <div class="titlebox_starts">						        <div class="titlebox_one"> Mobile </div>						        <div class="titlebox_two"> <%= f.text_field :mobile_number %> </div>					         </div>			                           <%= fields_for :user_detail do |user_detail| %>			     				             <div class="titlebox_starts">						         <div class="titlebox_one"> Marital Status </div>						         <div class="titlebox_two"> <%= user_detail.select :marital_status, options_for_select(%w(Single Married Divorced )) %> </div>					         </div>					         <div class="titlebox_starts">						         <div class="titlebox_one"> Occupation  </div>						         <div class="titlebox_two">  <%= user_detail.text_field :occupation %> </div>					         </div>					         <div class="titlebox_starts">						         <div class="titlebox_one"> Country of Origin  </div>						         <div class="titlebox_two"> <%= f.text_field :country %> </div>					         </div>                        <% end %>						 <br/>                                             <div class="contact_details"></div> <br/>                           <%= fields_for :user_detail do |user_detail| %>								<div class="titlebox_starts">                                     <div class="titlebox_one">Name </div>                                     <div class="titlebox_two"> <%= user_detail.text_field :emergency_contact_name %> </div>                                </div>								<div class="titlebox_starts">									 <div class="titlebox_one"> Relationship To Patient </div>									 <div class="titlebox_two"> <%= user_detail.text_field :emergency_contact_relation %> </div>								</div>								<div class="titleareass_starts">									 <div class="titleareass_one"> Address </div>									 <div class="titleareass_two"> <%= user_detail.text_field :emergency_contact_address %> </div>								</div>								<div class="titlebox_starts">									 <div class="titlebox_one"> Phone </div>									 <div class="titlebox_two"> <%= user_detail.text_field :emergency_contact_phone %> </div>								</div>                          <% end %>				                     <div class="update_but">	                  <%= image_submit_tag("/images/Family-Practice_update_butt_16.jpg", :width => "154",  :height => "32" ) %>	               </div>				   			</div>           	 <% end %>	  			 </div><script type="text/javascript">   jQuery(document).ready(function()	 {		jQuery("#user_detail_date_of_birth").datepicker(		 { 		   showOn: "button",		   buttonImage: "images/calendar.gif",		   buttonImageOnly: true,		   onSelect: populateList		 });   	 }); </script>